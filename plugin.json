{
  "name": "CodeCalc",
  "pluginName": "Code Calculator",
  "main": "src/index.html",
  "logo": "logo.png",
  "features": [
    {
      "code": "codecalc",
      "explain": "代码计算器",
      "cmds": ["Calculator", "CodeCalc", "Code计算器"]
    },
    {
      "code": "calculator",
      "explain": "计算算式",
      "icon": "logo-run.png",
      "cmds": [
        {
          "type": "regex",
          "label": "快速计算",
          "match": "/^(?:\\(*-?\\d+(\\.\\d+)?\\)* ?[+\\-*/%^] ?)+\\(*-?\\d+(\\.\\d+)?\\)*$/"
        }
      ]
    }
  ],
  "version": "1.0.1",
  "description": "一个可以运行代码的计算器"
} 